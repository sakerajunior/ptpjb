<script language="javascript">
	var iAppPublik_NumberOfTab=2;
	var iAppInternal_NumberOfTab=2;
	
	var iAppPublik_TabIndex=0;
	var iAppInternal_TabIndex=0;

	$(document).ready(function(){
		$("li.ants-social-media").mouseenter(function(){
			$(this).find("img").each(function(){
				$(this).animate({top:"0px"});
			});
		});
	
		$("li.ants-social-media").mouseleave(function(){
			$(this).find("img").each(function(){
				$(this).animate({top:"-26px"});
			});
		});
		
		//NAVIGASI LINK APLIKASI
		$("#AppNavAplikasiInternal li.AppBack").click(function(){
			if(iAppInternal_TabIndex>0) { BackAppSlide("AplikasiInternal"); }
			RefreshAppNav("AplikasiInternal");
		});
		$("#AppNavAplikasiInternal li.AppNext").click(function(){
			if(iAppInternal_TabIndex<iAppInternal_NumberOfTab-1) { NextAppSlide("AplikasiInternal"); }
			RefreshAppNav("AplikasiInternal");
		});
		//APLIKASI PUBLIK
		$("#AppNavAplikasiPublik li.AppBack").click(function(){
			if(iAppPublik_TabIndex>0) { BackAppSlide("AplikasiPublik"); }
			RefreshAppNav("AplikasiPublik");
		});
		$("#AppNavAplikasiPublik li.AppNext").click(function(){
			if(iAppPublik_TabIndex<iAppPublik_NumberOfTab-1) { NextAppSlide("AplikasiPublik"); }
			RefreshAppNav("AplikasiPublik");
		});
	});
	
	function RefreshAppNav(sComponentID) {
		if(sComponentID=="AplikasiInternal") {
			if(iAppInternal_TabIndex-1<0) { 
				$("#AplikasiInternal_Back").addClass('Disabled');
			} else {
				$("#AplikasiInternal_Back").removeClass('Disabled');
			}
	
			if(iAppInternal_TabIndex+1>=iAppInternal_NumberOfTab) {
				$("#AplikasiInternal_Next").addClass('Disabled');
			} else {
				$("#AplikasiInternal_Next").removeClass('Disabled');
			}
		} else {
			if(iAppPublik_TabIndex-1<0) { 
				$("#AplikasiPublik_Back").addClass('Disabled');
			} else {
				$("#AplikasiPublik_Back").removeClass('Disabled');
			}
	
			if(iAppPublik_TabIndex+1>=iAppPublik_NumberOfTab) {
				$("#AplikasiPublik_Next").addClass('Disabled');
			} else {
				$("#AplikasiPublik_Next").removeClass('Disabled');
			}
		}
	}
	
	function BackAppSlide(sComponentID) {
		if(sComponentID=="AplikasiInternal") {
			var ia=iAppInternal_TabIndex; var ib=ia-1;
			$("#vbAplikasiInternal"+ia).css({ 'left': '0px' });
			$("#vbAplikasiInternal"+ib).css({ 'left': '-336px' }); $("#vbAplikasiInternal"+ib).show();
	
			$("#vbAplikasiInternal"+ia).animate({left:"336"});
			$("#vbAplikasiInternal"+ib).animate({left:"0"});
			iAppInternal_TabIndex-=1;
		} else {
			var ia=iAppPublik_TabIndex; var ib=ia-1;
			$("#vbAplikasiPublik"+ia).css({ 'left': '0px' });
			$("#vbAplikasiPublik"+ib).css({ 'left': '-336px' }); $("#vbAplikasiPublik"+ib).show();
	
			$("#vbAplikasiPublik"+ia).animate({left:"336"});
			$("#vbAplikasiPublik"+ib).animate({left:"0"});
			iAppPublik_TabIndex-=1;
		}
	}
	
	function NextAppSlide(sComponentID) {
		if(sComponentID=="AplikasiInternal") {
			var ia=iAppInternal_TabIndex; var ib=ia+1;
			$("#vbAplikasiInternal"+ia).css({ 'left': '0px' });
			$("#vbAplikasiInternal"+ib).css({ 'left': '336px' }); $("#vbAplikasiInternal"+ib).show();
	
			$("#vbAplikasiInternal"+ia).animate({left:"-336"});
			$("#vbAplikasiInternal"+ib).animate({left:"0"});
			iAppInternal_TabIndex+=1;
		} else {
			var ia=iAppPublik_TabIndex; var ib=ia+1;
			$("#vbAplikasiPublik"+ia).css({ 'left': '0px' });
			$("#vbAplikasiPublik"+ib).css({ 'left': '336px' }); $("#vbAplikasiPublik"+ib).show();
	
			$("#vbAplikasiPublik"+ia).animate({left:"-336"});
			$("#vbAplikasiPublik"+ib).animate({left:"0"});
			iAppPublik_TabIndex+=1;
		}
	}
</script>

<script type="text/javascript">
$(window).scroll(function(e) {
	var BatasAnchor = $(".ants-newsticker-container").offset().top;
		
	if ($(this).scrollTop() >= BatasAnchor && $('#vbContainerMenu').css('position') != 'fixed') {
		$("#vbContainerMenu").find(".antsPopUpMenu").css({ "margin":"0 auto", "width":"1024px" });
		$("#vbContainerMenu").css({
			"position":"fixed", "top":"-80px", "opacity":"0", "border-bottom":"1px solid #ffffff" 
		}).animate({ "opacity":"0.98", "top":"0px" });
		
		$(".antsBackToTop").css({ "opacity":"0" }).show().animate({ "opacity":"0.5" });
	} 
	else if ($(this).scrollTop() < BatasAnchor && $('#vbContainerMenu').css('position') != 'relative') 
	{
		$("#vbContainerMenu").find(".antsPopUpMenu").css({ "margin":"0px", "width":"auto" });
		$('#vbContainerMenu').css({
			"position": "relative",
			"opacity": "1",
			"border-bottom":"none"
		});
		
		$(".antsBackToTop").hide();
	}
});
</script>
